{
  "buildCommand": "cd metaverse/apps/frontend && npm run build",
  "outputDirectory": "metaverse/apps/frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
